{
  "version": 2,
  "builds": [
    {
      "src": "app/server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/server.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./"
  },
  "regions": ["all"]
}
